<!DOCTYPE html>
<html lang="ru">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <!-- Meta, title, CSS, favicons, etc. -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>Деканат | </title>

    <!-- Bootstrap -->
    <link href="{{ asset('assets/vendor/bootstrap/dist/css/bootstrap.min.css') }}" rel="stylesheet">
    <!-- Font Awesome -->
    <link href="{{ asset('assets/vendor/font-awesome/css/font-awesome.min.css') }}" rel="stylesheet">
    {% block stylesheets %}

    {% endblock %}

    <!-- Custom Theme Style -->
    <link href="{{ asset('assets/css/custom.css') }}" rel="stylesheet">

</head>

<body class="nav-md">

<div class="container body">
    <div class="main_container">
        {% include 'sidebar.html.twig' %}
        {% include 'topnav.html.twig' %}
        <div class="right_col" role="main">
            <div>
                <div class="page-title">
                    <div class="title_left">
                        <h3>{% block page_title %}{% endblock %}</h3>
                    </div>
                </div>
                <div class="clearfix"></div>
                {% block page_content %}{% endblock %}</div>
        </div>
    </div>
    {% include 'footer.html.twig' %}
</div>


<!-- jQuery -->
<script src="{{ asset('assets/vendor/jquery/dist/jquery.min.js') }}"></script>
<!-- Bootstrap -->
<script src="{{ asset('assets/vendor/bootstrap/dist/js/bootstrap.min.js') }}"></script>
<!-- FastClick -->
<script src="{{ asset('assets/vendor/fastclick/lib/fastclick.js') }}"></script>
<!-- NProgress -->
<script src="{{ asset('assets/vendor/nprogress/nprogress.js') }}"></script>
{% block javascripts %}

{% endblock %}

<!-- Custom Theme Scripts -->
<script src="{{ asset('assets/js/custom.js') }}"></script>

